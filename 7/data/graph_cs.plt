
set term latex
set output "graph_cs.tex"
file = "< paste Cs.dat poz.dat"
t = 10*60.0

set ylabel '\popi{A}{Bq}'
set xlabel '\popi{E^\prime}{keV}'

set logscale y

f(x) = A1*exp(-(((x-B1)**2)/(2*C1**2)))
B1 = 110
A1 = 2
C1=3
fit [100:150] f(x) file u 1:(($2-$4)/t) via A1,B1,C1

g(x) = A2*exp(-(((x-B2)**2)/(2*C2**2)))
B2 = 70
A2 = 1
fit [60:100] g(x) file u 1:(($2-$4)/t) via A2,B2,C2

h(x) = A3*exp(-(((x-B3)**2)/(2*C3**2)))
B3 = 40
A3 = 1
fit [20:60] h(x) file u 1:((($2-$4)/t) -f($1)-g($1)) via A3,B3,C3

l(x) = f(x)+g(x)+h(x)

plot [24:150] [0.001:100] file u 1:(($2-$4)/t) t 'nameranÃ© spektrum', f(x) t '$f_1\(x\)$' #, g(x) t '$f_2\(x\)$'  , h(x) t '$f_3\(x\)$'  , l(x) t '$f_1\(x\)+f_2\(x\)+f_3\(x\)$'

show output
